{
  "name": "superoffice-tools",
  "displayName": "SuperOffice Tools",
  "version": "0.0.1",
  "description": "A Visual Studio Code extension for SuperOffice tools",
  "license": "ISC",
  "author": "Eivind Fasting",
  "publisher": "ejfasting",
  "main": "./out/extension.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/mocha": "catalog:",
    "@types/node": "catalog:",
    "@types/uuid": "^9.0.5",
    "@types/vscode": "catalog:",
    "uuid": "^9.0.1"
  },
  "contributes": {
    "commands": [
      {
        "command": "ejfasting.showGreeting",
        "title": "Hello World TS"
      },
      {
        "command": "ejfasting.authenticate",
        "title": "Authenticate with SuperOffice"
      }
    ]
  },
  "activationEvents": [
    "onUri"
  ],
  "engines": {
    "vscode": "^1.107.0"
  }
}
